datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model Food {
    id         Int       @id @default(autoincrement())
    title      String
    img        String
    author     String
    calories   Int       @default(0)
    created_at DateTime? @default(now())
}
